    "\"\"\"\n",
    "Purchase Pulse - Car Price Prediction System (Enhanced)\n"
    "Author: Pranav Rana\n",
    "\"\"\"\n",
    "\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from sklearn.model_selection import train_test_split, cross_val_score, KFold\n",
    "from sklearn.preprocessing import StandardScaler, OneHotEncoder, LabelEncoder\n",
    "from sklearn.compose import ColumnTransformer\n",
    "from sklearn.pipeline import Pipeline\n",
    "from sklearn.impute import SimpleImputer\n",
    "from sklearn.ensemble import RandomForestRegressor, GradientBoostingRegressor\n",
    "from sklearn.linear_model import Ridge, Lasso, ElasticNet\n",
    "from sklearn.metrics import mean_squared_error, r2_score, mean_absolute_error\n",
    "from sklearn.model_selection import GridSearchCV\n",
    "import warnings\n",
    "import tkinter as tk\n",
    "from tkinter import ttk, messagebox, filedialog\n",
    "import pickle\n",
    "import os\n",
    "from datetime import datetime, timedelta\n",
    "import json\n",
    "\n",
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "# Set random state for reproducibility\n",
    "RANDOM_STATE = 42\n",
    "np.random.seed(RANDOM_STATE)\n",
    "\n",
    "class PurchasePulse:\n",
    "    def __init__(self):\n",
    "        self.model = None\n",
    "        self.model_name = None\n",
    "        self.feature_columns = None\n",
    "        self.results = {}\n",
    "        \n",
    "        print(\"=\"*60)\n",
    "        print(\"PURCHASE PULSE - Car Price Prediction System\")\n",
    "        print(\"Author: Pranav\")\n",
    "        print(\"=\"*60)\n",
    "\n",
    "    def generate_sample_data(self):\n",
    "        \"\"\"Generate realistic sample car data for demonstration\"\"\"\n",
    "        print(\"\\n\" + \"=\"*50)\n",
    "        print(\"GENERATING SAMPLE CAR DATA\")\n",
    "        print(\"=\"*50)\n",
    "        \n",
    "        # Car manufacturers and their typical characteristics\n",
    "        manufacturers_data = {\n",
    "            'Toyota': {'luxury': False, 'price_range': (15, 45), 'reliability': 0.9},\n",
    "            'Honda': {'luxury': False, 'price_range': (18, 42), 'reliability': 0.85},\n",
    "            'Ford': {'luxury': False, 'price_range': (20, 55), 'reliability': 0.75},\n",
    "            'Chevrolet': {'luxury': False, 'price_range': (22, 60), 'reliability': 0.7},\n",
    "            'BMW': {'luxury': True, 'price_range': (35, 90), 'reliability': 0.8},\n",
    "            'Mercedes-Benz': {'luxury': True, 'price_range': (40, 120), 'reliability': 0.75},\n",
    "            'Audi': {'luxury': True, 'price_range': (38, 85), 'reliability': 0.78},\n",
    "            'Lexus': {'luxury': True, 'price_range': (35, 80), 'reliability': 0.9},\n",
    "            'Acura': {'luxury': True, 'price_range': (30, 65), 'reliability': 0.85},\n",
    "            'Nissan': {'luxury': False, 'price_range': (16, 40), 'reliability': 0.8},\n",
    "            'Hyundai': {'luxury': False, 'price_range': (15, 35), 'reliability': 0.82},\n",
    "            'Kia': {'luxury': False, 'price_range': (16, 38), 'reliability': 0.8},\n",
    "            'Volkswagen': {'luxury': False, 'price_range': (22, 50), 'reliability': 0.72},\n",
    "            'Subaru': {'luxury': False, 'price_range': (22, 45), 'reliability': 0.85},\n",
    "            'Mazda': {'luxury': False, 'price_range': (20, 42), 'reliability': 0.83}\n",
    "        }\n",
    "        \n",
    "        vehicle_types = ['Sedan', 'SUV', 'Hatchback', 'Coupe', 'Convertible', 'Wagon', 'Pickup']\n",
    "        \n",
    "        n_samples = 1000\n",
    "        data = []\n",
    "        \n",
    "        for i in range(n_samples):\n",
    "            manufacturer = np.random.choice(list(manufacturers_data.keys()))\n",
    "            mfg_data = manufacturers_data[manufacturer]\n",
    "            vehicle_type = np.random.choice(vehicle_types)\n",
    "            \n",
    "            # Generate correlated features\n",
    "            is_luxury = mfg_data['luxury']\n",
    "            base_price = np.random.uniform(*mfg_data['price_range'])\n",
    "            \n",
    "            # Engine size affects horsepower and price\n",
    "            engine_size = np.random.uniform(1.5, 6.0)\n",
    "            if is_luxury:\n",
    "                engine_size += np.random.uniform(0.5, 1.5)\n",
    "            \n",
    "            # Horsepower correlated with engine size\n",
    "            horsepower = engine_size * np.random.uniform(45, 80) + np.random.normal(0, 10)\n",
    "            horsepower = max(120, min(500, horsepower))\n",
    "            \n",
    "            # Vehicle dimensions\n",
    "            if vehicle_type == 'SUV':\n",
    "                length = np.random.uniform(185, 210)\n",
    "                width = np.random.uniform(72, 78)\n",
    "                wheelbase = np.random.uniform(110, 125)\n",
    "                curb_weight = np.random.uniform(3800, 5200)\n",
    "            elif vehicle_type == 'Sedan':\n",
    "                length = np.random.uniform(175, 195)\n",
    "                width = np.random.uniform(68, 74)\n",
    "                wheelbase = np.random.uniform(105, 115)\n",
    "                curb_weight = np.random.uniform(2800, 3800)\n",
    "            else:\n",
    "                length = np.random.uniform(165, 185)\n",
    "                width = np.random.uniform(66, 72)\n",
    "                wheelbase = np.random.uniform(100, 110)\n",
    "                curb_weight = np.random.uniform(2500, 3500)\n",
    "            \n",
    "            # Fuel efficiency inversely related to engine size and weight\n",
    "            fuel_efficiency = max(15, 45 - (engine_size * 3) - (curb_weight / 200) + np.random.normal(0, 2))\n",
    "            \n",
    "            # Fuel capacity\n",
    "            fuel_capacity = np.random.uniform(12, 22)\n",
    "            if vehicle_type == 'SUV':\n",
    "                fuel_capacity += np.random.uniform(3, 8)\n",
    "            \n",
    "            # Sales figures\n",
    "            sales = np.random.uniform(5, 150)\n",
    "            if manufacturer in ['Toyota', 'Honda', 'Ford']:\n",
    "                sales *= np.random.uniform(1.2, 2.0)\n",
    "            \n",
    "            # Vehicle age\n",
    "            launch_date = datetime.now() - timedelta(days=np.random.randint(0, 2555))  # 0-7 years\n",
    "            \n",
    "            # 4-year resale value\n",
    "            resale_value = base_price * np.random.uniform(0.4, 0.7) * mfg_data['reliability']\n",
    "            \n",
    "            # Price influenced by multiple factors\n",
    "            price_multiplier = 1.0\n",
    "            price_multiplier *= (1 + (horsepower - 180) / 500)  # Horsepower effect\n",
    "            price_multiplier *= (1 + (curb_weight - 3000) / 5000)  # Weight effect\n",
    "            if is_luxury:\n",
    "                price_multiplier *= np.random.uniform(1.3, 2.0)\n",
    "            \n",
    "            final_price = base_price * price_multiplier + np.random.normal(0, 3)\n",
    "            final_price = max(12, final_price)  # Minimum price\n",
    "            \n",
    "            data.append({\n",
    "                'Manufacturer': manufacturer,\n",
    "                'Vehicle type': vehicle_type,\n",
    "                'Sales in thousands': round(sales, 1),\n",
    "                '4-year resale value': round(resale_value, 1),\n",
    "                'Engine size': round(engine_size, 1),\n",
    "                'Horsepower': int(horsepower),\n",
    "                'Wheelbase': round(wheelbase, 1),\n",
    "                'Width': round(width, 1),\n",
    "                'Length': round(length, 1),\n",
    "                'Curb weight': int(curb_weight),\n",
    "                'Fuel capacity': round(fuel_capacity, 1),\n",
    "                'Fuel efficiency': round(fuel_efficiency, 1),\n",
    "                'Latest Launch': launch_date.strftime('%Y-%m-%d'),\n",
    "                'Price in thousands': round(final_price, 2)\n",
    "            })\n",
    "        \n",
    "        df = pd.DataFrame(data)\n",
    "        print(f\"✓ Generated {len(df)} sample car records\")\n",
    "        print(f\"✓ Manufacturers: {len(df['Manufacturer'].unique())}\")\n",
    "        print(f\"✓ Vehicle types: {len(df['Vehicle type'].unique())}\")\n",
    "        print(f\"✓ Price range: ${df['Price in thousands'].min():.1f}k - ${df['Price in thousands'].max():.1f}k\")\n",
    "        \n",
    "        # Save sample data\n",
    "        df.to_csv('sample_car_data.csv', index=False)\n",
    "        print(\"✓ Sample data saved as 'sample_car_data.csv'\")\n",
    "        \n",
    "        return df\n",
    "\n",
    "    def load_and_prepare_data(self, filename=None):\n",
    "        \"\"\"Load car sales dataset\"\"\"\n",
    "        try:\n",
    "            if filename and os.path.exists(filename):\n",
    "                df = pd.read_csv(filename)\n",
    "                print(f\"✓ Successfully loaded data from {filename}\")\n",
    "            elif os.path.exists('sample_car_data.csv'):\n",
    "                df = pd.read_csv('sample_car_data.csv')\n",
    "                print(\"✓ Loaded existing sample data\")\n",
    "            else:\n",
    "                print(\"⚠️  No data file found. Generating sample data...\")\n",
    "                df = self.generate_sample_data()\n",
    "            \n",
    "            return df\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"Error loading data: {str(e)}\")\n",
    "            print(\"Generating sample data instead...\")\n",
    "            return self.generate_sample_data()\n",
    "\n",
    "    def explore_data(self, df):\n",
    "        \"\"\"Explore and analyze the dataset\"\"\"\n",
    "        print(\"\\n\" + \"=\"*50)\n",
    "        print(\"DATASET EXPLORATION\")\n",
    "        print(\"=\"*50)\n",
    "        \n",
    "        print(f\"Dataset shape: {df.shape}\")\n",
    "        print(f\"Total records: {df.shape[0]}\")\n",
    "        print(f\"Total features: {df.shape[1]}\")\n",
    "        \n",
    "        print(\"\\nFirst 5 rows:\")\n",
    "        print(df.head())\n",
    "        \n",
    "        print(\"\\nDataset info:\")\n",
    "        print(df.info())\n",
    "        \n",
    "        print(\"\\nMissing values:\")\n",
    "        missing_values = df.isnull().sum()\n",
    "        missing_count = missing_values[missing_values > 0]\n",
    "        if len(missing_count) > 0:\n",
    "            print(missing_count)\n",
    "        else:\n",
    "            print(\"No missing values found!\")\n",
    "        \n",
    "        print(\"\\nTarget variable statistics (Price in thousands):\")\n",
    "        print(df['Price in thousands'].describe())\n",
    "        \n",
    "        print(f\"\\nManufacturers: {', '.join(sorted(df['Manufacturer'].unique()))}\")\n",
    "        print(f\"Vehicle types: {', '.join(sorted(df['Vehicle type'].unique()))}\")\n",
    "        \n",
    "        return df\n",
    "\n",
    "    def feature_engineering(self, df):\n",
    "        \"\"\"Create new features and prepare data for modeling\"\"\"\n",
    "        print(\"\\n\" + \"=\"*50)\n",
    "        print(\"FEATURE ENGINEERING\")\n",
    "        print(\"=\"*50)\n",
    "        \n",
    "        df_processed = df.copy()\n",
    "        \n",
    "        # Handle missing values in target variable\n",
    "        if df_processed['Price in thousands'].isnull().sum() > 0:\n",
    "            print(\"Removing rows with missing price values...\")\n",
    "            df_processed = df_processed.dropna(subset=['Price in thousands'])\n",
    "        \n",
    "        # Handle missing values in other columns\n",
    "        numerical_cols = ['4-year resale value', 'Engine size', 'Horsepower', 'Wheelbase', \n",
    "                         'Width', 'Length', 'Curb weight', 'Fuel capacity', 'Fuel efficiency']\n",
    "        \n",
    "        for col in numerical_cols:\n",
    "            if col in df_processed.columns and df_processed[col].isnull().sum() > 0:\n",
    "                median_val = df_processed[col].median()\n",
    "                df_processed[col].fillna(median_val, inplace=True)\n",
    "                print(f\"Filled missing values in {col} with median: {median_val}\")\n",
    "        \n",
    "        # Create engineered features\n",
    "        print(\"\\nCreating engineered features...\")\n",
    "        \n",
    "        # Power-to-weight ratio\n",
    "        df_processed['power_to_weight'] = df_processed['Horsepower'] / (df_processed['Curb weight'] / 1000)\n",
    "        \n",
    "        # Fuel efficiency category\n",
    "        df_processed['fuel_efficiency_category'] = pd.cut(\n",
    "            df_processed['Fuel efficiency'], \n",
    "            bins=[0, 20, 25, 30, 50], \n",
    "            labels=['Low', 'Medium', 'High', 'Very High']\n",
    "        )\n",
    "        \n",
    "        # Engine size category\n",
    "        df_processed['engine_category'] = pd.cut(\n",
    "            df_processed['Engine size'], \n",
    "            bins=[0, 2.0, 3.0, 4.0, 10], \n",
    "            labels=['Small', 'Medium', 'Large', 'Very Large']\n",
    "        )\n",
    "        \n",
    "        # Luxury brand indicator\n",
    "        luxury_brands = ['Acura', 'Audi', 'BMW', 'Cadillac', 'Infiniti', 'Jaguar', \n",
    "                         'Lexus', 'Lincoln', 'Mercedes-Benz', 'Porsche']\n",
    "        df_processed['is_luxury'] = df_processed['Manufacturer'].isin(luxury_brands).astype(int)\n",
    "        \n",
    "        # Vehicle age\n",
    "        df_processed['Latest Launch'] = pd.to_datetime(df_processed['Latest Launch'], errors='coerce')\n",
    "        current_date = datetime.now()\n",
    "        df_processed['vehicle_age_years'] = (current_date - df_processed['Latest Launch']).dt.days / 365.25\n",
    "        df_processed['vehicle_age_years'] = df_processed['vehicle_age_years'].fillna(\n",
    "            df_processed['vehicle_age_years'].median()\n",
    "        )\n",
    "        \n",
    "        # Performance score\n",
    "        df_processed['performance_score'] = (\n",
    "            df_processed['Horsepower'] * 0.4 + \n",
    "            df_processed['power_to_weight'] * 200 * 0.3 + \n",
    "            (50 - df_processed['Fuel efficiency']) * 0.3\n",
    "        )\n",
    "        \n",
    "        # Size category\n",
    "        df_processed['size_score'] = (\n",
    "            df_processed['Length'] * 0.4 + \n",
    "            df_processed['Width'] * 0.3 + \n",
    "            df_processed['Wheelbase'] * 0.3\n",
    "        )\n",
    "        \n",
    "        print(\"✓ Feature engineering completed\")\n",
    "        features_created = ['power_to_weight', 'fuel_efficiency_category', 'engine_category', \n",
    "                           'is_luxury', 'vehicle_age_years', 'performance_score', 'size_score']\n",
    "        print(f\"New features created: {', '.join(features_created)}\")\n",
    "        \n",
    "        return df_processed\n",
    "\n",
    "    def prepare_model_data(self, df):\n",
    "        \"\"\"Prepare data for machine learning models\"\"\"\n",
    "        print(\"\\n\" + \"=\"*50)\n",
    "        print(\"MODEL DATA PREPARATION\")\n",
    "        print(\"=\"*50)\n",
    "        \n",
    "        target = 'Price in thousands'\n",
    "        \n",
    "        feature_columns = [\n",
    "            'Sales in thousands', '4-year resale value', 'Engine size', 'Horsepower',\n",
    "            'Wheelbase', 'Width', 'Length', 'Curb weight', 'Fuel capacity', \n",
    "            'Fuel efficiency', 'power_to_weight', 'is_luxury', 'vehicle_age_years',\n",
    "            'performance_score', 'size_score', 'Manufacturer', 'Vehicle type'\n",
    "        ]\n",
    "        \n",
    "        # Filter features that exist in the dataset\n",
    "        available_features = [col for col in feature_columns if col in df.columns]\n",
    "        self.feature_columns = available_features\n",
    "        \n",
    "        X = df[available_features].copy()\n",
    "        y = df[target].copy()\n",
    "        \n",
    "        # Separate categorical and numerical features\n",
    "        categorical_features = ['Manufacturer', 'Vehicle type']\n",
    "        numerical_features = [col for col in available_features if col not in categorical_features]\n",
    "        \n",
    "        print(f\"Features selected: {len(available_features)}\")\n",
    "        print(f\"Numerical features: {len(numerical_features)}\")\n",
    "        print(f\"Categorical features: {len(categorical_features)}\")\n",
    "        print(f\"Target variable: {target}\")\n",
    "        \n",
    "        return X, y, numerical_features, categorical_features\n",
    "\n",
    "    def create_preprocessing_pipeline(self, numerical_features, categorical_features):\n",
    "        \"\"\"Create preprocessing pipeline\"\"\"\n",
    "        preprocessor = ColumnTransformer(\n",
    "            transformers=[\n",
    "                ('num', Pipeline(steps=[\n",
    "                    ('imputer', SimpleImputer(strategy='median')),\n",
    "                    ('scaler', StandardScaler())\n",
    "                ]), numerical_features),\n",
    "                ('cat', Pipeline(steps=[\n",
    "                    ('imputer', SimpleImputer(strategy='most_frequent')),\n",
    "                    ('onehot', OneHotEncoder(handle_unknown='ignore', sparse_output=False))\n",
    "                ]), categorical_features)\n",
    "            ])\n",
    "        \n",
    "        return preprocessor\n",
    "\n",
    "    def train_and_evaluate_models(self, X, y, preprocessor):\n",
    "        \"\"\"Train and evaluate multiple machine learning models\"\"\"\n",
    "        print(\"\\n\" + \"=\"*50)\n",
    "        print(\"MODEL TRAINING & EVALUATION\")\n",
    "        print(\"=\"*50)\n",
    "        \n",
    "        # Split data\n",
    "        X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.2, random_state=RANDOM_STATE)\n",
    "        print(f\"Training set size: {X_train.shape[0]} samples\")\n",
    "        print(f\"Test set size: {X_test.shape[0]} samples\")\n",
    "        \n",
    "        # Define models with optimized parameters\n",
    "        models = {\n",
    "            \"Ridge Regression\": Ridge(alpha=1.0, random_state=RANDOM_STATE),\n",
    "            \"Lasso Regression\": Lasso(alpha=0.1, random_state=RANDOM_STATE, max_iter=2000),\n",
    "            \"ElasticNet\": ElasticNet(alpha=0.1, l1_ratio=0.5, random_state=RANDOM_STATE, max_iter=2000),\n",
    "            \"Random Forest\": RandomForestRegressor(\n",
    "                n_estimators=100, max_depth=15, min_samples_leaf=3, \n",
    "                random_state=RANDOM_STATE, n_jobs=-1\n",
    "            ),\n",
    "            \"Gradient Boosting\": GradientBoostingRegressor(\n",
    "                n_estimators=100, max_depth=6, learning_rate=0.1,\n",
    "                random_state=RANDOM_STATE\n",
    "            )\n",
    "        }\n",
    "        \n",
    "        # Store results\n",
    "        results = {}\n",
    "        kfold = KFold(n_splits=5, shuffle=True, random_state=RANDOM_STATE)\n",
    "        \n",
    "        for name, model in models.items():\n",
    "            print(f\"\\n{'='*20} {name} {'='*20}\")\n",
    "            \n",
    "            # Create pipeline\n",
    "            pipeline = Pipeline(steps=[\n",
    "                ('preprocessor', preprocessor),\n",
    "                ('model', model)\n",
    "            ])\n",
    "            \n",
    "            # Cross-validation\n",
    "            cv_scores = cross_val_score(pipeline, X_train, y_train, cv=kfold, scoring='r2')\n",
    "            \n",
    "            # Train on full training set\n",
    "            pipeline.fit(X_train, y_train)\n",
    "            \n",
    "            # Predictions\n",
    "            y_train_pred = pipeline.predict(X_train)\n",
    "            y_test_pred = pipeline.predict(X_test)\n",
    "            \n",
    "            # Calculate metrics\n",
    "            train_r2 = r2_score(y_train, y_train_pred)\n",
    "            test_r2 = r2_score(y_test, y_test_pred)\n",
    "            test_rmse = np.sqrt(mean_squared_error(y_test, y_test_pred))\n",
    "            test_mae = mean_absolute_error(y_test, y_test_pred)\n",
    "            \n",
    "            # Store results\n",
    "            results[name] = {\n",
    "                'pipeline': pipeline,\n",
    "                'cv_r2_mean': cv_scores.mean(),\n",
    "                'cv_r2_std': cv_scores.std(),\n",
    "                'train_r2': train_r2,\n",
    "                'test_r2': test_r2,\n",
    "                'test_rmse': test_rmse,\n",
    "                'test_mae': test_mae\n",
    "            }\n",
    "            \n",
    "            # Print results\n",
    "            print(f\"Cross-validation R²: {cv_scores.mean():.4f} ± {cv_scores.std():.4f}\")\n",
    "            print(f\"Training R²: {train_r2:.4f}\")\n",
    "            print(f\"Test R²: {test_r2:.4f}\")\n",
    "            print(f\"Test RMSE: ${test_rmse:.2f}k\")\n",
    "            print(f\"Test MAE: ${test_mae:.2f}k\")\n",
    "            \n",
    "            # Check for overfitting\n",
    "            overfitting = train_r2 - test_r2\n",
    "            if overfitting > 0.1:\n",
    "                print(f\"⚠️  Potential overfitting detected (difference: {overfitting:.4f})\")\n",
    "            else:\n",
    "                print(f\"✓ Good generalization (difference: {overfitting:.4f})\")\n",
    "        \n",
    "        self.results = results\n",
    "        return results, X_test, y_test\n",
    "\n",
    "    def select_best_model(self):\n",
    "        \"\"\"Select the best performing model\"\"\"\n",
    "        print(\"\\n\" + \"=\"*50)\n",
    "        print(\"BEST MODEL SELECTION\")\n",
    "        print(\"=\"*50)\n",
    "        \n",
    "        # Rank models by test R²\n",
    "        model_rankings = [(name, metrics['test_r2']) for name, metrics in self.results.items()]\n",
    "        model_rankings.sort(key=lambda x: x[1], reverse=True)\n",
    "        \n",
    "        print(\"Model Rankings (by Test R²):\")\n",
    "        for i, (name, r2) in enumerate(model_rankings, 1):\n",
    "            print(f\"{i}. {name}: {r2:.4f}\")\n",
    "        \n",
    "        best_model_name = model_rankings[0][0]\n",
    "        best_model = self.results[best_model_name]['pipeline']\n",
    "        \n",
    "        print(f\"\\n🏆 Best Model: {best_model_name}\")\n",
    "        print(f\"Test R²: {self.results[best_model_name]['test_r2']:.4f}\")\n",
    "        print(f\"Test RMSE: ${self.results[best_model_name]['test_rmse']:.2f}k\")\n",
    "        print(f\"Cross-validation R²: {self.results[best_model_name]['cv_r2_mean']:.4f} ± {self.results[best_model_name]['cv_r2_std']:.4f}\")\n",
    "        \n",
    "        self.model = best_model\n",
    "        self.model_name = best_model_name\n",
    "        \n",
    "        return best_model, best_model_name\n",
    "\n",
    "    def save_model(self, filename='purchase_pulse_model.pkl'):\n",
    "        \"\"\"Save the trained model and metadata\"\"\"\n",
    "        try:\n",
    "            model_data = {\n",
    "                'model': self.model,\n",
    "                'model_name': self.model_name,\n",
    "                'feature_columns': self.feature_columns,\n",
    "                'results': self.results\n",
    "            }\n",
    "            \n",
    "            with open(filename, 'wb') as file:\n",
    "                pickle.dump(model_data, file)\n",
    "            print(f\"\\n✓ Model saved as '{filename}'\")\n",
    "            return filename\n",
    "        except Exception as e:\n",
    "            print(f\"❌ Error saving model: {str(e)}\")\n",
    "            return None\n",
    "\n",
    "    def load_model(self, filename='purchase_pulse_model.pkl'):\n",
    "        \"\"\"Load a trained model\"\"\"\n",
    "        try:\n",
    "            with open(filename, 'rb') as file:\n",
    "                model_data = pickle.load(file)\n",
    "            \n",
    "            self.model = model_data['model']\n",
    "            self.model_name = model_data.get('model_name', 'Unknown')\n",
    "            self.feature_columns = model_data.get('feature_columns', [])\n",
    "            self.results = model_data.get('results', {})\n",
    "            \n",
    "            print(f\"✓ Model loaded from '{filename}'\")\n",
    "            return True\n",
    "        except Exception as e:\n",
    "            print(f\"❌ Error loading model: {str(e)}\")\n",
    "            return False\n",
    "\n",
