    "            \n",
    "            # Input fields configuration\n",
    "            fields = [\n",
    "                (\"Manufacturer\", manufacturers, \"combobox\"),\n",
    "                (\"Vehicle Type\", vehicle_types, \"combobox\"),\n",
    "                (\"Sales (thousands)\", 50.0, \"entry\"),\n",
    "                (\"4-year Resale Value ($k)\", 15.0, \"entry\"),\n",
    "                (\"Engine Size (L)\", 2.5, \"entry\"),\n",
    "                (\"Horsepower\", 180, \"entry\"),\n",
    "                (\"Wheelbase (inches)\", 105.0, \"entry\"),\n",
    "                (\"Width (inches)\", 70.0, \"entry\"),\n",
    "                (\"Length (inches)\", 185.0, \"entry\"),\n",
    "                (\"Curb Weight (lbs)\", 3200, \"entry\"),\n",
    "                (\"Fuel Capacity (gallons)\", 17.0, \"entry\"),\n",
    "                (\"Fuel Efficiency (MPG)\", 25, \"entry\")\n",
    "            ]\n",
    "            \n",
    "            # Create input fields with clean styling\n",
    "            for i, (label, default, field_type) in enumerate(fields):\n",
    "                field_frame = tk.Frame(self.scrollable_frame, bg=\"#f8f9fa\")\n",
    "                field_frame.pack(fill=\"x\", pady=6, padx=20)\n",
    "                \n",
    "                # Label\n",
    "                label_widget = tk.Label(\n",
    "                    field_frame, \n",
    "                    text=label, \n",
    "                    font=(\"Segoe UI\", 10),\n",
    "                    fg=\"#2c3e50\", \n",
    "                    bg=\"#f8f9fa\",\n",
    "                    anchor=\"w\"\n",
    "                )\n",
    "                label_widget.pack(fill=\"x\", pady=(0, 3))\n",
    "                \n",
    "                # Input widget\n",
    "                if field_type == \"combobox\":\n",
    "                    var = tk.StringVar(value=default[0] if default else \"\")\n",
    "                    widget = ttk.Combobox(\n",
    "                        field_frame, \n",
    "                        textvariable=var, \n",
    "                        values=default,\n",
    "                        font=(\"Segoe UI\", 10),\n",
    "                        state=\"readonly\"\n",
    "                    )\n",
    "                else:\n",
    "                    if isinstance(default, float):\n",
    "                        var = tk.DoubleVar(value=default)\n",
    "                    else:\n",
    "                        var = tk.IntVar(value=default)\n",
    "                    widget = tk.Entry(\n",
    "                        field_frame, \n",
    "                        textvariable=var, \n",
    "                        bg=\"#ffffff\", \n",
    "                        fg=\"#2c3e50\",\n",
    "                        font=(\"Segoe UI\", 10),\n",
    "                        relief=\"solid\",\n",
    "                        bd=1,\n",
    "                        highlightthickness=1,\n",
    "                        highlightcolor=\"#3498db\",\n",
    "                        highlightbackground=\"#bdc3c7\"\n",
    "                    )\n",
    "                \n",
    "                widget.pack(fill=\"x\", ipady=3)\n",
    "                self.vars[label] = var\n",
    "        \n",
    "        def create_charts_section(self, parent):\n",
