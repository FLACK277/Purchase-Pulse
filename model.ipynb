    "            ax.set_title('Top Manufacturers by Model Count', fontsize=12, color='#2c3e50', pad=20)\n",
    "            \n",
    "            # Set x-axis labels with rotation\n",
    "            ax.set_xticks(range(len(top_brands)))\n",
    "            ax.set_xticklabels(top_brands.index, rotation=45, ha='right', fontsize=9, color='#2c3e50')\n",
    "            \n",
    "            # Clean styling\n",
    "            ax.spines['top'].set_visible(False)\n",
    "            ax.spines['right'].set_visible(False)\n",
    "            ax.spines['left'].set_color('#bdc3c7')\n",
    "            ax.spines['bottom'].set_color('#bdc3c7')\n",
    "            ax.tick_params(colors='#7f8c8d', labelsize=9)\n",
    "            ax.grid(True, alpha=0.3, color='#ecf0f1', axis='y')\n",
    "            \n",
    "            plt.tight_layout()\n",
    "            \n",
    "            canvas = FigureCanvasTkAgg(fig, parent)\n",
    "            canvas.draw()\n",
    "            canvas.get_tk_widget().pack(fill=\"both\", expand=True, padx=10, pady=10)\n",
    "        \n",
