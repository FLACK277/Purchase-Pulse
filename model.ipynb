    "        \n",
    "        def create_price_distribution_chart(self, parent):\n",
    "            fig, ax = plt.subplots(figsize=(5, 4), facecolor='white')\n",
    "            \n",
    "            # Create price distribution histogram\n",
    "            prices = self.sample_data['Price in thousands'].dropna()\n",
    "            ax.hist(prices, bins=20, color='#3498db', alpha=0.7, edgecolor='white')\n",
    "            ax.set_xlabel('Price (thousands $)', fontsize=10, color='#2c3e50')\n",
    "            ax.set_ylabel('Frequency', fontsize=10, color='#2c3e50')\n",
    "            ax.set_title('Car Price Distribution', fontsize=12, color='#2c3e50', pad=20)\n",
    "            \n",
    "            # Clean styling\n",
    "            ax.spines['top'].set_visible(False)\n",
    "            ax.spines['right'].set_visible(False)\n",
    "            ax.spines['left'].set_color('#bdc3c7')\n",
    "            ax.spines['bottom'].set_color('#bdc3c7')\n",
    "            ax.tick_params(colors='#7f8c8d', labelsize=9)\n",
    "            ax.grid(True, alpha=0.3, color='#ecf0f1')\n",
    "            \n",
    "            plt.tight_layout()\n",
    "            \n",
    "            canvas = FigureCanvasTkAgg(fig, parent)\n",
    "            canvas.draw()\n",
    "            canvas.get_tk_widget().pack(fill=\"both\", expand=True, padx=10, pady=10)\n",
    "        \n",
    "        def create_correlation_chart(self, parent):\n",
    "            fig, ax = plt.subplots(figsize=(5, 4), facecolor='white')\n",
    "            \n",
    "            # Select numeric columns for correlation\n",
    "            numeric_cols = ['Price in thousands', 'Engine size', 'Horsepower', 'Fuel efficiency', 'Curb weight']\n",
    "            corr_data = self.sample_data[numeric_cols].corr()\n",
    "            \n",
    "            # Create heatmap\n",
    "            im = ax.imshow(corr_data, cmap='RdYlBu_r', aspect='auto', vmin=-1, vmax=1)\n",
    "            \n",
    "            # Set ticks and labels\n",
    "            ax.set_xticks(range(len(numeric_cols)))\n",
    "            ax.set_yticks(range(len(numeric_cols)))\n",
    "            ax.set_xticklabels([col.replace(' in thousands', '').replace(' ', '\\n') for col in numeric_cols], \n",
    "                              fontsize=8, color='#2c3e50')\n",
    "            ax.set_yticklabels([col.replace(' in thousands', '').replace(' ', '\\n') for col in numeric_cols], \n",
    "                              fontsize=8, color='#2c3e50')\n",
    "            \n",
    "            # Add correlation values\n",
    "            for i in range(len(numeric_cols)):\n",
    "                for j in range(len(numeric_cols)):\n",
    "                    text = ax.text(j, i, f'{corr_data.iloc[i, j]:.2f}',\n",
    "                                 ha=\"center\", va=\"center\", color='white' if abs(corr_data.iloc[i, j]) > 0.5 else 'black',\n",
    "                                 fontsize=8)\n",
    "            \n",
    "            ax.set_title('Feature Correlation Matrix', fontsize=12, color='#2c3e50', pad=20)\n",
    "            \n",
    "            plt.tight_layout()\n",
    "            \n",
    "            canvas = FigureCanvasTkAgg(fig, parent)\n",
    "            canvas.draw()\n",
    "            canvas.get_tk_widget().pack(fill=\"both\", expand=True, padx=10, pady=10)\n",
    "        \n",
    "        def create_brand_analysis_chart(self, parent):\n",
    "            fig, ax = plt.subplots(figsize=(5, 4), facecolor='white')\n",
    "            \n",
    "            # Get top 8 manufacturers by count\n",
